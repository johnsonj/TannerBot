
@{
    ViewBag.Title = "ImportData";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Import Data</h2>

<div>
@using (Html.BeginForm("OnImportData", "UserContext", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
        <label for="file">Filename:</label>
        <input type="file" name="file" id="file" />

        <input type="submit" />
}
</div>